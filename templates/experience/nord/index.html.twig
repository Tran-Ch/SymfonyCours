        desc: "Capitale du Vietnam, centre politique, culturel et économique du pays."
      },
      vn_hai_phong: {
        name: "Hai Phong",
        area: "1 561 km²",
        population: "≈ 2,1 millions",
        desc: "Grande ville portuaire du Nord, célèbre pour Cat Ba et Do Son."
      },
      vn_quang_ninh: {
        name: "Quang Ninh",
        area: "6 102 km²",
        population: "≈ 1,3 million",
        desc: "Connue pour la baie d’Ha Long, classée au patrimoine mondial de l’UNESCO."
      },
      vn_bac_ninh: {
        name: "Bac Ninh",
        area: "822 km²",
        population: "≈ 1,4 million",
        desc: "Terre du chant Quan Ho, riche en villages d’artisanat traditionnel."
      },
      vn_hung_yen: {
        name: "Hung Yen",
        area: "930 km²",
        population: "≈ 1,2 million",
        desc: "Réputée pour ses litchis et ses nombreux sites historiques."
      },
      vn_ninh_binh: {
        name: "Ninh Binh",
        area: "1 387 km²",
        population: "≈ 1 million",
        desc: "Abrite Trang An et Tam Coc–Bich Dong, classés au patrimoine mondial."
      },
      vn_phu_tho: {
        name: "Phu Tho",
        area: "3 532 km²",
        population: "≈ 1,4 million",
        desc: "Terre des rois Hùng, lieu sacré de la dynastie fondatrice du Vietnam."
      },
      vn_son_la: {
        name: "Son La",
        area: "14 125 km²",
        population: "≈ 1,2 million",
        desc: "Province montagneuse célèbre pour la région de Moc Chau, au climat agréable."
      },
      vn_dien_bien: {
        name: "Dien Bien",
        area: "9 541 km²",
        population: "≈ 615 000",
        desc: "Lieu de la bataille historique de Dien Bien Phu, riche en culture ethnique."
      },
      vn_lai_chau: {
        name: "Lai Chau",
        area: "9 068 km²",
        population: "≈ 480 000",
        desc: "Région montagneuse du Nord-Ouest, connue pour ses paysages grandioses."
      },
      vn_lao_cai: {
        name: "Lao Cai",
        area: "6 364 km²",
        population: "≈ 730 000",
        desc: "Ville frontalière et porte d’accès à Sa Pa, station climatique réputée."
      },
      vn_tuyen_quang: {
        name: "Tuyen Quang",
        area: "5 867 km²",
        population: "≈ 800 000",
        desc: "Considérée comme la « capitale de la zone libérée » durant la résistance."
      },
      vn_thai_nguyen: {
        name: "Thai Nguyen",
        area: "3 562 km²",
        population: "≈ 1,3 million",
        desc: "Célèbre pour son thé Tan Cuong et ses grandes zones industrielles."
      },
      vn_cao_bang: {
        name: "Cao Bang",
        area: "6 703 km²",
        population: "≈ 530 000",
        desc: "Connue pour les chutes de Ban Gioc et ses nombreux sites historiques."
      },
      vn_lang_son: {
        name: "Lang Son",
        area: "8 310 km²",
        population: "≈ 790 000",
        desc: "Province frontalière, centre majeur du commerce với la Chine."
      }
    };

    window.addEventListener('load', () => {
      const tooltip = document.getElementById('tooltip');
      const card   = document.getElementById('city-info');
      const nameEl = card.querySelector('.city-name');
      const areaEl = card.querySelector('.city-area');
      const popEl  = card.querySelector('.city-population');
      const descEl = card.querySelector('.city-desc');

      // Hover + click trên các tỉnh
      document.querySelectorAll('.province').forEach(province => {
        // Hover: tooltip
        province.addEventListener('mousemove', e => {
          const name = province.dataset.name || province.id;
          tooltip.textContent = name;
          tooltip.style.left = (e.clientX + 10) + 'px';
          tooltip.style.top  = (e.clientY + 10) + 'px';
          tooltip.style.opacity = 1;
        });

        province.addEventListener('mouseleave', () => {
          tooltip.style.opacity = 0;
        });

        // Click: hiện card thông tin
        province.addEventListener('click', () => {
          const data = provinceData[province.id];
          if (!data) return;

          card.classList.remove('d-none');
          nameEl.textContent = data.name;
          areaEl.textContent = data.area;
          popEl.textContent  = data.population;
          descEl.textContent = data.desc;
        });
      });

      // ===== CLICK RA NGOÀI MAP / NGOÀI CARD THÌ ẨN CARD =====
      document.addEventListener('click', (e) => {
        if (e.target.closest('#city-info') || e.target.closest('.province')) {
          return;
        }
        card.classList.add('d-none');
      });
    });
  </script>
