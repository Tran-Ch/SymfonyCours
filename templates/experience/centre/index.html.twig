{% extends 'base.html.twig' %}

{% block title %}Expérience du Centrale{% endblock %}

{% block stylesheets %}
  {{ parent() }}
  <style>
    body {
        background-color: rgba(36, 75, 68, 0.38);
    }

    main {
        margin-top: 130px;
        padding: 40px 20px 60px 20px;
        background-color: transparent;
    }

    .centre-wrapper {
        max-width: 1600px;
        margin: 0 auto;
        display: grid;
        grid-template-columns: minmax(0, 1.4fr) minmax(0, 1fr);
        gap: 40px;
        align-items: flex-start;
    }

    .centre-map-card {
        background-color: transparent;
        border-radius: 20px;
        padding: 0;
        box-shadow: none;
        display: flex;
        flex-direction: column;
        align-items: center;
    }

    .centre-right {
        display: flex;
        flex-direction: column;
        gap: 24px;
        color: #f3f9c5;
    }

    .centre-right h1 {
        font-size: 26px;
        line-height: 1.4;
        margin: 0;
        color: #f3f9c5;
    }

    .centre-btn-box {
        background-color: rgba(255,255,255,0.22);
        border-radius: 16px;
        padding: 18px 20px;
        display: flex;
        flex-direction: column;
        gap: 10px;
    }

    /* NÚT CHÍNH CỦA CENTRE – LÀM CHUẨN MÀU CHO 3 MIỀN */
    .centre-main-btn {    
        background-color: rgba(216, 228, 159, 1);
        border-radius: 12px;
        padding: 10px 18px;
        text-align: center;
        font-weight: bold;
        border: none;
        cursor: pointer;
        font-size: 14px;
        transition: background-color 0.3s ease, transform 0.2s ease;
    }

    .centre-main-btn:hover {
      background-color: #fada7b;
      transform: translateY(-1px);
    }

    .centre-intro {
        font-size: 14px;
        color: #dbe8d2;
        line-height: 1.6;
    }

    .centre-description {
        font-size: 16px;
        color: #6d8778;
        line-height: 1.6;
    }

    .centre-description strong {
        font-size: 20px;
        color: #fef9b8;
        display: block;
        margin-bottom: 6px;
    }

    .map-section {
        width: 100%;
        display: flex;
        justify-content: center;
        align-items: flex-start;
        padding-top: 20px;
        background-color: transparent;
        border-radius: 16px;
        overflow: visible;
    }

    .map-container {
        position: relative;
        display: block;
        width: 100%;
        max-width: 1200px;
        margin-top: -150px;
    }

    .map-container svg {
        display: block;
        width: 100%;
        height: auto;
    }

    .province > g path {
      transition: fill 0.25s ease, opacity 0.25s ease, stroke-width 0.25s ease;
    }

    .province:hover > g path {
      opacity: 0.9;
      fill: #ffe066;
      stroke-width: 0.35;
      cursor: pointer;
    }

    .province > path {
        transition: fill 0.25s ease;
    }

    .province:hover > path {
        fill: #c97a38;
    }

    #tooltip {
        position: fixed;
        background: rgba(0,0,0,0.8);
        color: #fff;
        padding: 4px 8px;
        border-radius: 4px;
        font-size: 12px;
        pointer-events: none;
        opacity: 0;
        white-space: nowrap;
        transition: opacity 0.1s ease;
        z-index: 9999;
    }

    .city-info-wrapper {
        width: 100%;
        max-width: 480px;
        margin: 5px auto 0;
        display: flex;
        justify-content: center;
    }

    #city-info.card {
        max-width: 100%;
        border-radius: 8px;
        overflow: hidden;
        box-shadow: 0 2px 6px rgba(0,0,0,0.15);
        border: none;
    }

    #city-info .card-header {
        background-color: #406459 !important;
        color: #ffffff;
        font-weight: 600;
        font-size: 16px;
        padding: 8px 16px;
    }

    #city-info .card-body {
        background-color: #ffffff;
        padding: 12px 20px;
    }

    #city-info .list-group-item {
        border-left: 0;
        border-right: 0;
        border-color: #e0e0e0;
        font-size: 14px;
        padding: 8px 0;
    }

    #city-info .list-group-item:first-child {
        border-top: 0;
    }

    #city-info .list-group-item strong {
        font-weight: 600;
    }

    #city-info .city-desc {
        margin-top: 4px;
        line-height: 1.5;
    }

    .card-header {
      background-color: #406459 !important;
    }

    @media (max-width: 1200px) {
        .centre-wrapper {
            grid-template-columns: minmax(0, 1.1fr) minmax(0, 1fr);
            gap: 30px;
        }

        .map-container {
            max-width: 520px;
        }
    }

    @media (max-width: 992px) {
        .centre-wrapper {
            grid-template-columns: 1fr;
            margin: 0 auto;
        }

        .centre-right h1 {
            text-align: center;
        }

        .map-container {
            max-width: 420px;
            margin: 0 auto;
        }

        .city-info-wrapper {
            max-width: 480px;
            margin: 16px auto 0;
            justify-content: center;
        }
    }

    @media (max-width: 768px) {
        main {
            margin-top: 120px;
            padding: 30px 10px 40px 10px;
        }

        .map-section {
            justify-content: center;
            padding-top: 10px;
        }

        .map-container {
            max-width: 360px;
        }

        .city-info-wrapper {
            max-width: 100%;
            padding: 0 8px;
        }
    }

    @media (max-width: 576px) {
      main {
        margin-top: 110px;
        padding: 24px 8px 36px 8px;
      }

      .centre-right h1 {
        font-size: 22px;
      }
    }
  </style>
{% endblock %}

