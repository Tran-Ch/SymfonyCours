{% extends 'base.html.twig' %}

{% block title %}Expérience du Nord{% endblock %}

{% block stylesheets %}
  {{ parent() }}
  <style>
    /* tránh tràn ngang cho mọi kích thước */
    html, body {
      overflow-x: hidden;
    }

    body {
        background-color: rgba(36, 75, 68, 0.38);
    }

    main {
        margin-top: 130px;
        padding: 40px 20px 60px 20px;
        background-color: transparent;
    }

    /* ===== LAYOUT 2 CỘT: MAP TRÁI – NỘI DUNG PHẢI ===== */
    .nord-wrapper {
        max-width: 1200px;
        margin: 0 auto;
        display: grid;
        grid-template-columns: minmax(0, 1.15fr) minmax(0, 1fr);
        gap: 40px;
        align-items: flex-start;
    }

    /* cột trái: map + card tỉnh – xếp dọc, căn giữa theo ngang */
    .nord-map-card {
        background-color: transparent;
        border-radius: 20px;
        padding: 0;
        box-shadow: none;
        display: flex;
        flex-direction: column;
        align-items: center; /* card nằm giữa map theo trục ngang */
    }

    .nord-right {
        display: flex;
        flex-direction: column;
        gap: 24px;
        color: #f3f9c5;
    }

    .nord-right h1 {
        font-size: 26px;
        line-height: 1.4;
        margin: 0;
        color: #f3f9c5;
    }

    .nord-btn-box {
        background-color: rgba(255,255,255,0.22);
        border-radius: 16px;
        padding: 18px 20px;
        display: flex;
        flex-direction: column;
        gap: 10px;
    }

    .nord-main-btn {
        background-color: rgba(216, 228, 159, 1);
        border-radius: 12px;
        padding: 10px 18px;
        text-align: center;
        font-weight: bold;
        border: none;
        cursor: pointer;
        font-size: 14px;
        transition: background-color 0.3s ease, transform 0.2s ease;
    }

    .nord-main-btn:hover {
      background-color: #fada7b;
      transform: translateY(-1px);
    }

    /* đoạn giới thiệu ngắn (đoạn số 1) */
    .nord-intro {
        font-size: 14px;
        color: #dbe8d2;      /* màu nhạt hơn, bạn có thể đổi */
        line-height: 1.6;
    }

    /* đoạn mô tả dài (đoạn số 2) */
    .nord-description {
        font-size: 16px;
        color: #6d8778;      /* màu sáng hơn */
        line-height: 1.6;
    }

    .nord-description strong {
        font-size: 20px;
        color: #fef9b8;
        display: block;
        margin-bottom: 6px;
    }

    /* ===== PHẦN MAP SVG + CARD TỈNH ===== */

    .map-section {
      width: 100%;
      height: auto;
      display: flex;
      justify-content: center;
      align-items: flex-start;
      padding-top: 20px;
      background-color: transparent;
      border-radius: 16px;
      overflow: visible;
    }

    .map-container {
      position: relative;
      display: inline-block;
      background: transparent;
      padding: 0;
      border-radius: 0;
      box-shadow: none;
      overflow: visible;
      margin-top: -25px;   /* kéo map lên một chút dưới header (màn hình lớn) */
    }

    .map-container svg {
      display: block;
      width: 330px;        /* kích thước logic ban đầu */
      height: auto;
      transform: scale(2.5);          /* tỷ lệ map bạn đang dùng (màn hình lớn) */
      transform-origin: top left;
      margin-left: -600px;
    }

    /* hiệu ứng hover tỉnh */
    .province > g path {
      transition: fill 0.25s ease, opacity 0.25s ease, stroke-width 0.25s ease;
    }

    .province:hover > g path {
      opacity: 0.9;
      fill: #ffe066;
      stroke-width: 0.35;
      cursor: pointer;
    }

    /* chữ label tỉnh */
    .province > path {
        transition: fill 0.25s ease;
    }

    .province:hover > path {
        fill: #c97a38;
    }

    /* tooltip tên tỉnh */
    #tooltip {
        position: fixed;
        background: rgba(0,0,0,0.8);
        color: #fff;
        padding: 4px 8px;
        border-radius: 4px;
        font-size: 12px;
        pointer-events: none;
        opacity: 0;
        white-space: nowrap;
        transition: opacity 0.1s ease;
        z-index: 9999;
    }

    /* card thông tin tỉnh thành – nằm NGAY dưới map và ở giữa (layout màn lớn) */
    .city-info-wrapper {
        width: 1000px;        /* cùng chiều rộng với .container */
        max-width: 95%;
        margin-left: -250px;   /* cùng offset với .container để card nằm dưới map */
        margin-top: 120px;     /* tăng/giảm để card thấp xuống đúng mức bạn muốn */
        display: flex;         /* dùng flex để canh giữa card  */
        justify-content: center;  /* card nằm giữa theo trục ngang */
    }

    /* Khung card tổng thể */
    #city-info.card {
        max-width: 380px;         /* kích thước card giống hình cũ */
        width: 100%;              /* cho phép co giãn nhỏ hơn 380px trên mobile */
        border-radius: 8px;
        overflow: hidden;         /* bo tròn cả header + thân */
        box-shadow: 0 2px 6px rgba(0,0,0,0.15);
        border: none;
    }

    /* Header màu xanh đậm, chữ trắng, căn trái */
    #city-info .card-header {
      background-color: #406459 !important; /* giống màu bạn đang dùng */
      color: #ffffff;
      font-weight: 600;
      font-size: 16px;
      padding: 8px 16px;
    }

    /* Phần body trắng */
    #city-info .card-body {
      background-color: #ffffff;
      padding: 12px 20px;
    }

    /* Các dòng thông tin bên trong */
    #city-info .list-group-item {
      border-left: 0;
      border-right: 0;
      border-color: #e0e0e0;        /* màu đường kẻ mảnh giữa các dòng */
      font-size: 14px;
      padding: 8px 0;
    }

    /* bỏ đường kẻ trên cùng để giống hình */
    #city-info .list-group-item:first-child {
      border-top: 0;
    }

    /* tiêu đề từng dòng (Superficie, Population, Description…) */
    #city-info .list-group-item strong {
      font-weight: 600;
    }

    /* đoạn mô tả dài */
    #city-info .city-desc {
      margin-top: 4px;
      line-height: 1.5;
    }

    .card-header {
      background-color: #406459 !important;
    }

    /* ===== RESPONSIVE ===== */

    /* Màn từ 1200–1440px (14" thường nằm ở đây): giảm nhẹ scale map */
    @media (max-width: 1440px) {
      .nord-wrapper {
        max-width: 1100px;
      }

      .map-container svg {
        transform: scale(2.1);   /* nhỏ hơn 2.5 để bớt tràn */
        margin-left: -450px;     /* chỉnh lại cho map nằm đúng vị trí */
      }

      .city-info-wrapper {
        width: 900px;
        margin-left: -200px;
      }
    }

    /* Tablet/laptop vừa (≤ 1200px): giảm nhẹ gap, vẫn giữ layout 2 cột */
    @media (max-width: 1200px) {
      .nord-wrapper {
        grid-template-columns: minmax(0, 1.1fr) minmax(0, 1fr);
        gap: 30px;
      }

      .map-container svg {
        transform: scale(1.9);
        margin-left: -250px;
      }
    }

    /* Tablet dọc / màn nhỏ hơn (≤ 992px): map + card xếp dọc, ôm trong màn hình */
    @media (max-width: 992px) {
      .nord-wrapper {
        grid-template-columns: 1fr;   /* map trên, text dưới */
        gap: 24px;
      }

      .nord-right h1 {
        text-align: center;
      }

      .map-section {
        justify-content: center;
        align-items: center;
        flex-direction: column;  /* map trên, card dưới */
      }

      .map-container {
        width: 100%;
        display: flex;
        justify-content: center; /* luôn căn giữa map */
      }

      .map-container svg {
        width: 100%;
        height: auto;
        transform: scale(1.5);            /* map to hơn */
        transform-origin: center top;     /* tâm nằm giữa để không bị lệch */
        
        margin-left: 0 !important;        /* xoá đẩy lệch trái */
        max-width: 330px;                 /* giới hạn để không tràn */
      }

      .city-info-wrapper {
        margin-left: 0 !important;
        margin-right: 0;
        justify-content: center;
      }
    }

    /* Điện thoại (≤ 768px): thu nhỏ thêm nhưng giữ khoảng cách & vị trí */
    @media (max-width: 768px) {
      main {
        margin-top: 120px;
        padding: 30px 10px 40px 10px;
      }

      .map-section {
        justify-content: center;
        padding-top: 10px;
      }

      .map-container {
        width: 100%;
        display: flex;
        justify-content: center;
      }

      .map-container svg {
        width: 100%;
        transform: scale(1.4);
        transform-origin: center top;

        margin-left: 0 !important; /* luôn giữa */
        max-width: 280px;          /* phù hợp màn nhỏ */
      }

      .city-info-wrapper {
        margin: 20px auto 0;
        justify-content: center;
      }
    }

    /* Màn rất nhỏ (≤ 576px): co thêm 1 chút */
    @media (max-width: 576px) {
      main {
        margin-top: 110px;
        padding: 24px 8px 36px 8px;
      }

      .nord-right h1 {
        font-size: 22px;
      }

      .map-container,
      .map-container svg,
      .city-info-wrapper {
        max-width: 320px;
      }
    }
  </style>
{% endblock %}


