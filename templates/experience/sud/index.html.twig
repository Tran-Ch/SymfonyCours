{% block javascripts %}
    {{ parent() }}
        <script>
            // ====== DỮ LIỆU CÁC TỈNH ======
            const provinceData = {
            vn_an_giang: {
                name: "An Giang",
                area: "9 889 km²",
                population: "≈ 4,95 triệu",
                desc: "Province frontalière du delta du Mékong, célèbre pour ses forêts de cajeputiers, ses villages flottants et sa diversité culturelle avec les communautés Khmer, Cham et Hoa."
            },

            vn_ca_mau: {
                name: "Cà Mau",
                area: "5 295 km²",
                population: "≈ 1,2 triệu",
                desc: "Province la plus méridionale du Vietnam, réputée pour ses mangroves, son écotourisme et le cap de Cà Mau, point ultime du pays."
            },

            vn_can_tho: {
                name: "Cần Thơ",
                area: "≈ 6 361 km²",
                population: "≈ 1,23 triệu",
                desc: "Ville centrale du delta du Mékong, cœur économique et culturel de la région, connue pour ses marchés flottants et ses vergers luxuriants."
            },

            vn_vinh_long: {
                name: "Vĩnh Long",
                area: "≈ 6 296 km²",
                population: "≈ 4,25 triệu",
                desc: "Province entre les bras du Mékong, appréciée pour ses jardins fruitiers, ses villages artisanaux et son atmosphère paisible au fil de l’eau."
            },

            vn_dong_thap: {
                name: "Đồng Tháp",
                area: "≈ 3 300 km²",
                population: "≈ 1,7 triệu",
                desc: "Province fluviale renommée pour ses champs de lotus, ses vergers de mandarines et la réserve naturelle de Tràm Chim."
            },

            vn_tay_ninh: {
                name: "Tây Ninh",
                area: "≈ 8 536 km²",
                population: "≈ 3,25 triệu",
                desc: "Province frontalière du Cambodge, centre spirituel du Caodaïsme, avec des paysages emblématiques comme la montagne Bà Đen."
            },

            vn_ho_chi_minh: {
                name: "TP. Hồ Chí Minh",
                area: "≈ 2 095 km²",
                population: "≈ 9,3 triệu",
                desc: "Plus grande métropole du Vietnam, centre économique et culturel du Sud, mêlant modernité vibrante et héritage colonial français."
            },

            vn_dong_nai: {
                name: "Đồng Nai",
                area: "≈ 12 737 km²",
                population: "≈ 4,49 triệu",
                desc: "Province industrielle dynamique voisine de Hồ Chí Minh-Ville, connue pour le parc national de Cát Tiên et ses villages traditionnels."
            }
            };

            window.addEventListener('load', () => {
            const tooltip = document.getElementById('tooltip');
            const card   = document.getElementById('city-info');
            const nameEl = card.querySelector('.city-name');
            const areaEl = card.querySelector('.city-area');
            const popEl  = card.querySelector('.city-population');
            const descEl = card.querySelector('.city-desc');

            // Hover + click trên các tỉnh
            document.querySelectorAll('.province').forEach(province => {
                // Hover: tooltip
                province.addEventListener('mousemove', e => {
                const name = province.dataset.name || province.id;
                tooltip.textContent = name;
                tooltip.style.left = (e.clientX + 10) + 'px';
                tooltip.style.top  = (e.clientY + 10) + 'px';
                tooltip.style.opacity = 1;
                });

                province.addEventListener('mouseleave', () => {
                tooltip.style.opacity = 0;
                });

                // Click: hiện card thông tin
                province.addEventListener('click', () => {
                const data = provinceData[province.id];
                if (!data) return;

                card.classList.remove('d-none');
                nameEl.textContent = data.name;
                areaEl.textContent = data.area;
                popEl.textContent  = data.population;
                descEl.textContent = data.desc;
                });
            });

            // ===== CLICK RA NGOÀI MAP / NGOÀI CARD THÌ ẨN CARD =====
            document.addEventListener('click', (e) => {
                if (e.target.closest('#city-info') || e.target.closest('.province')) {
                return;
                }
                card.classList.add('d-none');
            });
            });
        </script>
