        vn_truong_sa: {
            name: "Trường Sa",
            area: "496 km²",
            population: "≈ 93 người",
            desc: "Archipel stratégique en mer de l’Est, administré par la province de Khánh Hòa, avec une présence symbolique vietnamienne."
        },

        vn_hoang_sa: {
            name: "Hoàng Sa",
            area: "305 km²",
            population: "Không có cư dân thường trú",
            desc: "Archipel rattaché administrativement à la ville de Đà Nẵng, actuellement occupé illégalement par la Chine depuis 1974."
        },

        vn_lam_dong: {
            name: "Lâm Đồng",
            area: "≈ 9 356 km²",
            population: "≈ 3,9 triệu",
            desc: "Haut plateau verdoyant du Sud, abritant Đà Lạt – ville romantique au climat frais et aux paysages brumeux."
        },

        vn_khanh_hoa: {
            name: "Khánh Hòa",
            area: "≈ 5 218 km²",
            population: "≈ 1,5 triệu",
            desc: "Destination balnéaire phare avec Nha Trang, ses plages turquoise et ses îles paradisiaques."
        },

        vn_daklak: {
            name: "Đắk Lắk",
            area: "≈ 13 125 km²",
            population: "≈ 2,3 triệu",
            desc: "Province du Tây Nguyên, dont le chef-lieu est Buôn Ma Thuột, véritable terre du café et des ethnies montagnardes, avec des forêts profondes et des traditions vibrantes"
        },

        vn_gia_lai: {
            name: "Gia Lai",
            area: "≈ 15 537 km²",
            population: "≈ 1,6 triệu",
            desc: "Vaste province du Tây Nguyên, la deuxième plus grande du Vietnam, connue pour ses lacs volcaniques et sa culture autochtone, où vivent de nombreuses ethnies montagnardes telles que les Jrai et les Bahnar."
        },

        vn_quang_ngai: {
            name: "Quảng Ngãi",
            area: "≈ 5 155 km²",
            population: "≈ 1,6 triệu",
            desc: "Côtière et authentique, célèbre pour l’île de Lý Sơn et les vestiges de la civilisation Sa Huỳnh."
        },

        vn_da_nang: {
            name: "Đà Nẵng",
            area: "≈ 1 285 km²",
            population: "≈ 1,4 triệu",
            desc: " Ville dynamique du Centre du Vietnam, directement administrée par l’État, véritable cœur économique et touristique de la région, entre mer et montagnes, réputée pour son architecture moderne, ses plages, ainsi que des symboles emblématiques comme le pont du Dragon et la péninsule de Sơn Trà."
        },

        vn_hue: {
            name: "Huế",
            area: "≈ 83 km² (thành phố), ≈ 5 048 km² (tỉnh Thừa Thiên Huế)",
            population: "≈ 652 000 (thành phố), ≈ 1,1 triệu (tỉnh)",
            desc: "Ancienne capitale impériale, berceau des trésors culturels de la dynastie Nguyễn."
        },

        vn_quang_tri: {
            name: "Quảng Trị",
            area: "≈ 4 740 km²",
            population: "≈ 608 000",
            desc: "Terre de mémoire, marquée par l’histoire de la guerre et les paysages paisibles du Nord-Centre."
        },

        vn_ha_tinh: {
            name: "Hà Tĩnh",
            area: "≈ 5 998 km²",
            population: "≈ 1,3 triệu",
            desc: "Province discrète mais poétique, avec des plages sauvages et des villages traditionnels."
        },

        vn_nghe_an: {
            name: "Nghệ An",
            area: "≈ 16 487 km²",
            population: "≈ 3 triệu",
            desc: "Plus grande province du pays, patrie de Hồ Chí Minh, entre montagnes, mer et culture populaire."
        }
    };

    window.addEventListener('load', () => {
      const tooltip = document.getElementById('tooltip');
      const card   = document.getElementById('city-info');
      const nameEl = card.querySelector('.city-name');
      const areaEl = card.querySelector('.city-area');
      const popEl  = card.querySelector('.city-population');
      const descEl = card.querySelector('.city-desc');

      // Hover + click trên các tỉnh
      document.querySelectorAll('.province').forEach(province => {
        // Hover: tooltip
        province.addEventListener('mousemove', e => {
          const name = province.dataset.name || province.id;
          tooltip.textContent = name;
          tooltip.style.left = (e.clientX + 10) + 'px';
          tooltip.style.top  = (e.clientY + 10) + 'px';
          tooltip.style.opacity = 1;
        });

        province.addEventListener('mouseleave', () => {
          tooltip.style.opacity = 0;
        });

        // Click: hiện card thông tin
        province.addEventListener('click', () => {
          const data = provinceData[province.id];
          if (!data) return;

          card.classList.remove('d-none');
          nameEl.textContent = data.name;
          areaEl.textContent = data.area;
          popEl.textContent  = data.population;
          descEl.textContent = data.desc;
        });
      });

      // ===== CLICK RA NGOÀI MAP / NGOÀI CARD THÌ ẨN CARD =====
      document.addEventListener('click', (e) => {
        if (e.target.closest('#city-info') || e.target.closest('.province')) {
          return;
        }
        card.classList.add('d-none');
      });
    });
  </script>
